{
  "name": "my-perf-stats-service",
  "version": "1.0.0",
  "description": "A repository statistics service for my performance",
  "license": "ISC",
  "author": "Alex Figliolia",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npx ts-packager -e src/Start.ts",
    "certs": "tsx devtools/certs-generator/CLI.ts",
    "generate-types": "tsx devtools/codegen/CLI.ts",
    "dev": "tsx devtools/dev-server/CLI.ts",
    "lint": "tsx ci/commands/Lint.ts",
    "prod": "pm2 start 'npm run start' -i max",
    "prod:stop": "pm2 stop all",
    "start": "node dist/mjs/Start.js",
    "test": "jest"
  },
  "dependencies": {
    "@alexfigliolia/my-performance-async": "^1.0.5",
    "@alexfigliolia/my-performance-clients": "^1.0.0",
    "@figliolia/child-process": "^1.0.3",
    "body-parser": "^1.20.2",
    "chalk": "^4",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "graphql": "^16.0.0",
    "graphql-request": "^6.1.0",
    "node-fetch": "^3.3.2",
    "spdy": "^4.0.2"
  },
  "devDependencies": {
    "@figliolia/ts-packager": "^1.0.6",
    "@figliolia/typescript-dev-server": "^1.0.4",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/spdy": "^3.4.9",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "pm2": "^5.3.1",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "graphql": "^16.0.0"
  }
